        <arguments>
          <argument>
            <kind>1</kind>
            <string>
    Gravity = 0;
    switch(Icon){
        case consMonitorRing:
            global.Rings += 10;
            with(global.Player[0]){
                if(RingStereoMix == true){
                    audio_stop_sound(sndObjectRing);
                    audio_play_sound(sndObjectRing, 1, false);
                }else{
                    switch(RingStereoMix){
                        case 0:
                            audio_stop_sound(sndRingLeft);
                            audio_play_sound(sndRingLeft, 1, false)
                            RingStereoMix = 0.5;
                            break;
                        case 0.5:
                            audio_stop_sound(sndRingRight);
                            audio_play_sound(sndRingRight, 1, false)
                            RingStereoMix = 0;
                            break;
                    }                       
                }
            }
            if(global.Rings &gt;= global.RingsLife){
                global.Lives += 1;
                global.RingsLife += 100;
                audio_play_sound(sndLife, 1, false);
                if(audio_is_playing(sndInvincibility) == false)
                    audio_pause_sound(global.CurrentSong);
                else
                    audio_sound_gain(sndInvincibility, 0, 0);

                objController.LifeMusic = true;
            }
            break;
        case consMonitorEggman:
            with(PlayerId)
                scrPlayerHurt(other.id, sndPlayerDead);
            break;
        case consMonitorSpeedSneakers:
            with(objPlayer){
                SpeedSneakers       = true;
                SpeedSneakersTimer  = 1200;
                if(audio_is_playing(sndSuperSonic) == false){
                    audio_pause_sound(ds_map_find_value(LvlMusic, room));
                    audio_stop_sound(sndInvincibility);
                    audio_stop_sound(sndSpeedSneakers);
                    audio_play_sound(sndSpeedSneakers, 1, false);
                    if(audio_is_playing(sndLife))
                        audio_sound_gain(sndSpeedSneakers, 0, 0);
                    global.CurrentSong = sndSpeedSneakers;
                }
            }
            break;
        case consMonitorInvincibility:
            with(objPlayer){
                Invincibility       = 2;
                InvincibilityTimer  = 1200;
                if(audio_is_playing(sndSuperSonic) == false){
                    audio_pause_sound(ds_map_find_value(LvlMusic, room));
                    audio_stop_sound(sndSpeedSneakers);
                    audio_stop_sound(sndInvincibility);
                    audio_play_sound(sndInvincibility, 1, false);
                    if(audio_is_playing(sndLife))
                        audio_sound_gain(sndInvincibility, 0, 0);
                    global.CurrentSong = sndInvincibility;
                }
            }
            break;
        case consMonitorShield:
            with(global.Player[0]){
                Shield = consShieldNormal;
                audio_play_sound(sndShield, 1, false);
            }
            break;
        case consMonitorShieldWater:
            with(global.Player[0]){
                Shield = consShieldWater;
                audio_play_sound(sndShieldBubble, 1, false);
            }
            break;
        case consMonitorShieldElectric:
            with(global.Player[0]){
                Shield = consShieldElectric;
                audio_play_sound(sndShieldLightning, 1, false);
            }
            break;
        case consMonitorShieldFire:
            with(global.Player[0]){
                Shield = consShieldFire;
                audio_play_sound(sndShieldFire, 1, false);
            }
            break;
        case consMonitorLife:
            global.Lives += 1;
            audio_play_sound(sndLife, 1, false);
            if(audio_is_playing(sndInvincibility) == false)
                audio_pause_sound(global.CurrentSong);
            else
                audio_sound_gain(sndInvincibility, 0, 0);
            objController.LifeMusic = true;
            break;
    }
</string>
